<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_e_sublimation_category_list" model="ir.ui.view">
        <field name="name">view.e_sublimation.category.list</field>
        <field name="model">e_sublimation.category</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="view_e_sublimation_category_form" model="ir.ui.view">
        <field name="name">view.e_sublimation.category.form</field>
        <field name="model">e_sublimation.category</field>
        <field name="arch" type="xml">
            <form string="E_sublimation Category">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_e_sublimation_category" model="ir.actions.act_window">
        <field name="name">E_sublimation Category</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">e_sublimation.category</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new E_sublimation Category.
            </p>
        </field>
    </record>

</odoo>
