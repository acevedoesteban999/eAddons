<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="e_module_update_module_update_manual_wizard_view_form" model="ir.ui.view">
        <field name="name">e_module_update.module_update_manual_wizard.view.form</field>
        <field name="model">e_module_update.module_update_manual_wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="module_update_id" invisible="1"></field>
                        <field name="zip_file" widget="file"></field>
                    </group>
                </sheet>
                <footer>
                    <button string="Upload" name="action_upload" type="object" class="oe_highlight"/>
                    <button string="Cancel" special="cancel" class="oe_link"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>